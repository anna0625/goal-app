{
  "expo": {
    "name": "RNCourse",
    "slug": "RNCourse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/goal2.png",
      "resizeMode": "contain",
      "backgroundColor": "#eeeeee"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/goal2.png",
        "backgroundColor": "#EEEEEE"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
